{
  "name": "monorepo-starter-app",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "dev:web": "turbo run dev --filter=@app/web",
    "dev:admin": "turbo run dev --filter=@app/admin",
    "dev:server": "turbo run dev --filter=@app/server",
    "dev:frontend": "turbo run dev --filter=@app/web --filter=@app/admin",
    "build:web": "turbo run build --filter=@app/web",
    "build:admin": "turbo run build --filter=@app/admin",
    "build:server": "turbo run build --filter=@app/server",
    "clean": "turbo run clean && rm -rf node_modules",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "18.6.0",
    "@eslint/js": "^9.33.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.3.0",
    "@types/papaparse": "^5.3.16",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@unocss/transformer-directives": "^65.4.3",
    "@unocss/vite": "^66.4.2",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^1.0.0",
    "chalk": "^5.6.2",
    "commitizen": "^4.3.0",
    "commitlint": "^18.2.0",
    "cz-git": "^1.11.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "execa": "^9.6.1",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "less": "^4.4.1",
    "lint-staged": "^15.4.3",
    "papaparse": "^5.5.3",
    "prettier": "^3.6.2",
    "run-script-webpack-plugin": "^0.2.3",
    "tsx": "^4.20.5",
    "turbo": "^2.5.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "unocss": "^65.4.3",
    "vite": "^6.3.5",
    "webpack": "^5.105.1",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=22"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,yaml,yml,html,css,scss}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
